<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0" metadata-complete="false">

	<display-name>IBM_emp_01MVC_Basic_css</display-name>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>

<!-- 老師範例預備 -->
	<resource-ref>
		<description>DB Connection</description>
		<res-ref-name>jdbc/TestDB</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>

<!-- 我們專案共用的連線池 -->
	<resource-ref>
		<description>DB Connection for DA106G5</description>
		<res-ref-name>jdbc/DA106G5_DB</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>


<!-- 戍乂責任區 開始-->

	<!-- 地標開始 -->
		<servlet>
			<servlet-name>LocationTest</servlet-name>
			<servlet-class>com.location.model.LocationTest</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>LocationTest</servlet-name>
			<url-pattern>/LocationTest</url-pattern>
		</servlet-mapping>
	
	
		<servlet>
			<servlet-name>LocationServlet</servlet-name>
			<servlet-class>com.location.controller.LocationServlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>LocationServlet</servlet-name>
			<url-pattern>/front_end/location/location.do</url-pattern>
		</servlet-mapping>
	
	
		<servlet>
			<servlet-name>LocationServletAll</servlet-name>
			<servlet-class>com.location.controller.LocationServlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>LocationServletAll</servlet-name>
			<url-pattern>/location/location.do</url-pattern>
		</servlet-mapping>
		
		
		<servlet>
			<servlet-name>DBGifReader4Location</servlet-name>
			<servlet-class>com.location.model.DBGifReader4Location</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>DBGifReader4Location</servlet-name>
			<url-pattern>/DBGifReader4Location</url-pattern>
		</servlet-mapping>
	<!-- 地標結束 -->
	
	<!-- 天氣開始 -->
		<servlet>
			<servlet-name>Weather_detailTest</servlet-name>
			<servlet-class>com.weather_detail.model.Weather_detailTest</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>Weather_detailTest</servlet-name>
			<url-pattern>/Weather_detailTest</url-pattern>
		</servlet-mapping>
	
	
		<servlet>
			<servlet-name>Weather_detailServlet</servlet-name>
			<servlet-class>com.weather_detail.controller.Weather_detailServlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>Weather_detailServlet</servlet-name>
			<url-pattern>/front_end/weather_detail/weather_detail.do</url-pattern>
		</servlet-mapping>
	
	
		<servlet>
			<servlet-name>Weather_detailServletAll</servlet-name>
			<servlet-class>com.weather_detail.controller.Weather_detailServlet</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>Weather_detailServlet</servlet-name>
			<url-pattern>/weather_detail/weather_detail.do</url-pattern>
		</servlet-mapping>
		
		
		<servlet>
			<servlet-name>DBGifReader4Weather</servlet-name>
			<servlet-class>com.weather.model.DBGifReader4Weather</servlet-class>
		</servlet>
		<servlet-mapping>
			<servlet-name>DBGifReader4Weather</servlet-name>
			<url-pattern>/DBGifReader4Weather</url-pattern>
		</servlet-mapping>
	<!-- 天氣結束 -->

